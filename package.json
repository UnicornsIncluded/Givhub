    "@mapbox/mapbox-gl-directions": "^4.0.3",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "bulma": "0.8.1",
    "chalk": "^4.0.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "connect-mongo": "^3.2.0",
    "connected-react-router": "^6.8.0",
    "core-js": "^3.6.4",
    "date-fns": "^2.11.1",
